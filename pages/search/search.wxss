.sort-box .item.active.asc{
    background: transparent url(data:image/png;base64,) no-repeat;
    background-size: 45rpx 51rpx;
    background-position:right;
}

.sort-box .item.active.desc{
    background: transparent url(data:image/png;base64,) no-repeat;
    background-size: 45rpx 51rpx;
    background-position:right;
}

.sort-box-category{
    background: #fff;
    width: 100%;
    height: auto;
    overflow: hidden;
    padding: 40rpx 40rpx 0 0;
    border-bottom: 1px solid #d9d9d9;
}

.sort-box-category .item{
    height: 54rpx;
    line-height: 54rpx;
    text-align: center;
    float: left;
    padding: 0 16rpx;
    margin: 0 0 40rpx 40rpx;
    border: 1px solid #666;
    color: #333;
    font-size: 24rpx;
}

.sort-box-category .item.active{
    color: #b4282d;
    border: 1px solid #b4282d;
}

.cate-item{
    margin-top: 175rpx;
    height: auto;
    overflow: hidden;
}

.cate-item .h{
    height: 145rpx;
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.cate-item .h .name{
    display: block;
    height: 35rpx;
    margin-bottom: 18rpx;
    font-size: 30rpx;
    color: #333;
}

.cate-item .h .desc{
    display: block;
    height: 24rpx;
    font-size: 24rpx;
    color: #999;
}

.cate-item .b{
  width: 750rpx;
  padding: 0 6.25rpx;
  height: auto;
  overflow: hidden;
}

.cate-item .list-filter{
    height: 80rpx;
    width: 100%;
    background: #fff;
    margin-bottom: 6.25rpx;
}

.cate-item .b .item{
  float: left;
  background: #fff;
  width: 365rpx;
  margin-bottom: 6.25rpx;
  padding-bottom: 33.333rpx;
  height: auto;
  overflow: hidden;
  text-align: center;
}

.cate-item .b .item-b{
  margin-left: 6.25rpx;
}

.cate-item .item .img{
  width: 302rpx;
  height: 302rpx;
}

.cate-item .item .name{
  display: block;
  width: 365.625rpx;
  height: 35rpx;
  margin: 11.5rpx 0 22rpx 0;
  text-align: center;
  overflow: hidden;
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #333;
}

.cate-item .item .price{
  display: block;
  width: 365.625rpx;
  height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #b4282d;
}

.search-result-empty{
    width: 100%;
    height: 100%;
    padding-top: 300rpx;
}

.search-result-empty .icon{
    margin: 0 auto;
    display: block;
    width: 240rpx;
    height: 240rpx;
}

.search-result-empty .text{
    display: block;
    width: 100%;
    height: 40rpx;
    font-size: 28rpx;
    text-align: center;
    color: #999;
}
.search-now-type{
    margin:15rpx;
    height:60rpx;
    padding:10rpx 0rpx;
    font-size:35rpx;
}

/* 结果列表*/
.img {
    /* border-radius: 50%; */
    height: 100%;
    width: 100%;
}

.wrap-img {
    height: 150rpx;
    width: 150rpx;
}
.search-result{
    background-color: #ffffff;
    margin-top: 169rpx;
}
.searchRes .item {
    font-size: 34rpx;
    padding: 10rpx 0 10rpx 30rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    border:2rpx solid #d1d1d1;
    margin: 15rpx;
}
.item-title{
    display:flex;
    justify-content:space-between;
    align-items:center;
    width:100%;
    margin-bottom:16rpx;
    font-weight: bold;
}

.item-desc {
    margin-left: 40rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
    width: 500rpx;
    padding-bottom:20rpx;
}

.item-desc-each {
    width: 100%;
    display: flex;
    margin-top:20rpx;
}